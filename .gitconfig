[user]
	name = Kyle Kelly
	email = strifey16@gmail.com
[branch]
	autosetuprebase = always
[pull]
	rebase = preserve
[rebase]
	stat = true
	autostash = true
[alias]
	pog = log --pretty=format:\"%C(white)%h %Cblue%>(12)%ad %Cgreen%<(7)%aN%C(yellow)%d %Creset%s\"
	brog = "!git --no-pager log --pretty=format:\"%C(white)%h %Cblue%>(12)%cr %Cgreen%<(7)%aN%C(yellow)%d %Creset%s\" master..HEAD"
	lol = log --oneline --graph --decorate --color=always --pretty=format:'%C(auto)%h - %C(bold blue)%aN%Cgreen(%cr)%C(auto)%d%Creset %s' "$@"
	reci = commit --amend --no-edit
[push]
	followTags = true
[include]
	path = ~/.gitconfig_extra
